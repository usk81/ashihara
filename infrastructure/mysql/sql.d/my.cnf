[mysqld]
character-set-server=utf8mb4
collation-server=utf8mb4_general_ci
explicit-defaults-for-timestamp=1
log-bin=/var/log/mysql/bin-log
general-log=1
general-log-file=/var/log/mysql/mysqld.log

[mysql]
default-character-set=utf8mb4
character-set-server=utf8mb4

[client]
default-character-set=utf8mb4
